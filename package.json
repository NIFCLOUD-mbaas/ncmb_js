{
  "name": "ncmb",
  "version": "1.0.0",
  "description": "JavaScript SDK for NiftyCloud Mobile Backend as a Service",
  "main": "ncmb.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "nohup npm run stub:start & sleep 5 && NODE_ENV=test mocha -R list test/**_test.js && npm run cov:80 && npm run stub:stop",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- -R spec test/**_test.js",
    "coverage_": "nohup npm run stub:start & sleep 5 && NODE_ENV=test istanbul cover _mocha -- -R spec test/**_test.js && npm run stub:stop",
    "cov:80": "npm run coverage 2>&1 | grep Lines | perl -nle 'my ($l, $c, $p, @d) = split(/[\\s%]+/); exit(($p > 80) == 0)'",
    "test-one": "NODE_ENV=test mocha -R spec ",
    "integral_test": "NODE_ENV=integral gulp test",
    "build": "browserify -t babelify lib/ncmb.js -o ncmb.js",
    "stub:start": "stubcell -e test/mbaas.yml --basepath test/stub -p 18000",
    "stub:stop": "kill `ps aux | grep stubc[e]ll | awk '{print $2}'`"
  },
  "repository": "http://example.com",
  "readmefile": "README.md",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.1.8",
    "babelify": "^6.0.2",
    "beefy": "^2.1.5",
    "bluebird": "^2.9.12",
    "browserify": "^8.1.3",
    "chai": "^2.1.0",
    "coffee-script": "^1.9.1",
    "config": "^1.12.0",
    "create-error": "^0.3.1",
    "gulp": "^3.8.11",
    "gulp-browserify": "^0.5.1",
    "gulp-mocha": "^2.0.0",
    "gulp-stubcell": "^0.7.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "istanbul": "^0.3.14",
    "lodash": "^3.9.3",
    "mocha": "^2.1.0",
    "node-localstorage": "0.5.1",
    "qs": "^2.4.1",
    "request": "^2.53.0",
    "stubcell": "^0.8.0",
    "superagent": "^0.21.0",
    "superagent-proxy": "^0.3.1",
    "url": "^0.10.3"
  }
}
